    <script>
    var subd = '';

        var stack_bar_top = {"dir1": "down", "dir2": "right", "push": "top", "spacing1": 0, "spacing2": 0};
    function reperr()
    {
	var form = $('#repform')
	$.post('/ajax.php', {
		op :	'reperr',
		id :	$('input[name="id"]', form).val(),
		email :	$('input[name="email"]', form).val(),
		error :	$('textarea', form).val(),
	    },
	    function(e){
		$('#report').modal('hide')
		$.pnotify({
		    addclass:	'stack-bar-top',
		    width:	'100%',
		    stack:	stack_bar_top,
		    delay:	1000,
		    history:	false,
		    hide:	true,
		    closer:	false,
		    sticker:	false,
		    type:	'success',
		    icon:	false,
		    text:	'<h2 class="cntr">Thank You!</h2>'
		})
	    }
	)
    }

    function autocom(query, process)
    {
        if (!'http://www.'.match(new RegExp('^'+query, 'i')))
	{
	    $.getJSON('/ahead', {
		q: query
	    }, process)
	}
    }

    $(function(){
	    $('[rel="popover"]').popover({ trigger : 'hover', html: true})
    $('#srch').typeahead({source : autocom, items : 6, minLength : 3})
        $('#repform').on('submit', false)
    })
    </script>
